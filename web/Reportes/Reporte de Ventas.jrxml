	<subDataset name="PieChartDataset" uuid="f6fff52e-69f6-413d-835b-678f6cb64123"/>
	<queryString language="SQL">
		<![CDATA[SELECT
     venta.`idVent` AS venta_idVent,
     venta.`idPed` AS venta_idPed,
     venta.`fechaReal` AS venta_fechaReal,
     venta.`fechaEntreg` AS venta_fechaEntreg,
     venta.`idmed` AS venta_idmed,
     venta.`idmet` AS venta_idmet,
     venta.`dirEnvio` AS venta_dirEnvio,
     pedido.`idPed` AS pedido_idPed,
     pedido.`subtotal` AS pedido_subtotal,
     pedido.`descuento` AS pedido_descuento,
     pedido.`total` AS pedido_total,
     mediopago.`idmed` AS mediopago_idmed,
     mediopago.`medioPago` AS mediopago_medioPago,
     metodoenvio.`idmet` AS metodoenvio_idmet,
     metodoenvio.`metdEnvio` AS metodoenvio_metdEnvio
FROM
     `pedido` pedido INNER JOIN `venta` venta ON pedido.`idPed` = venta.`idPed`
     INNER JOIN `mediopago` mediopago ON venta.`idmed` = mediopago.`idmed`
     INNER JOIN `metodoenvio` metodoenvio ON venta.`idmet` = metodoenvio.`idmet`]]>
	</queryString>
